    "test": "./node_modules/.bin/mocha -r ts-node/register 'test/**/*.ts'"
  },
  "engines": {
    "handel": "*"
  },
  "dependencies": {
    "aws-sdk": "^2.266.1",
    "handel-extension-api": "^0.31.1",
    "handel-extension-support": "^0.31.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.4",
    "@types/sinon": "^4.3.1",
    "cpy-cli": "^2.0.0",
    "del-cli": "^1.1.0",
    "chai": "^4.1.2",
    "mocha": "^5.0.5",
    "sinon": "^4.5.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1"
  }
}
